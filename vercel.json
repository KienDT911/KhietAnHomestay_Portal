{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend_AdminManagement/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "backend/server.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "frontend_AdminManagement/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "frontend_AdminManagement/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "frontend_AdminManagement/js/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "frontend_AdminManagement/$1"
    },
    {
      "src": "/",
      "dest": "frontend_AdminManagement/index.html"
    }
  ]
}
